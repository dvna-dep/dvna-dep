<form>
    <div class="form-check">
        <% for (var rating in ratings) { %>
            <div>
                <input class="form-check-input" type="radio" name="securityRating" id=<%= "rating" + rating %>  value= <%=rating%> >
                <label class="form-check-label" for=<%= "rating" + rating %>>
                    <%= "Level " + rating %>
                </label>
            </div>
        <% } %>
    </div>
    <div>
        <button type="submit">Submit</button>
    </div>
</form>